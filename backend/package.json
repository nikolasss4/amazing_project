{
  "name": "risklaba-backend",
  "version": "1.0.0",
  "description": "Backend API for RiskLaba social trading community platform",
  "main": "dist/app.js",
  "scripts": {
    "dev": "tsx watch src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "worker:ingestion": "tsx src/workers/ingestion.worker.ts",
    "worker:leaderboard": "tsx src/workers/leaderboard.worker.ts",
    "worker:pulse": "tsx src/workers/pulse.worker.ts",
    "ingest:x": "tsx src/run-x-ingestion.ts",
    "build:narratives": "tsx src/run-build-narratives.ts",
    "test:x-provider": "tsx src/test-rapidapi-provider.ts",
    "test:x-narratives": "tsx src/test-x-narratives.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "trading",
    "social",
    "community",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@types/form-data": "^2.2.1",
    "bull": "^4.11.5",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "form-data": "^4.0.5",
    "ioredis": "^5.3.2",
    "node-fetch": "^2.7.0",
    "openai": "^6.16.0",
    "undici": "^7.18.2",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bull": "^4.10.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prisma": "^5.7.1",
    "ts-jest": "^29.4.6",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
